<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="apis.amapv2.com.res">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.WindowNoTitleActivity"
            android:theme="@style/WindowNoTitleActivityTheme" />
        <activity
            android:name=".activity.WindowFullScreenActivity"
            android:theme="@style/WindowFullScreenActivityTheme" />
        <activity
            android:name=".activity.WindowContentOverlayActivity"
            android:theme="@style/WindowContentOverlayTheme" />
        <activity android:name=".activity.WindowBackgroundActivity"
            android:theme="@style/WindowBackgroundTheme"
            />
    </application>

</manifest>